FORMAT = {(OPTIONS, WS, PLAYER) | PLAYER, (WS, PLAYERS) | NONE}
PLAYERS = {PLAYER, (WS, PLAYERS) | NONE}
PLAYER = {(OPTION_N, WS) | NONE, PATH}
OPTIONS = {OPTION_A | OPTION_V | OPTION_G | OPTION_D | OPTION_O | OPTION_S, (WS, OPTIONS) | NONE}
OPTION_A = {OPT_A}
OPTION_V = {OPT_V | OPT_VERBOSE, WS, VERBOSE}
OPTION_G = {OPT_G | OPT_GRAPHIC}
OPTION_O = {OPT_O, WS, PATH}
OPTION_N = {OPT_N, WS, NUMBER}
OPTION_D = {OPT_D | OPT_DUMP, WS, NUMBER}
OPTION_S = {OPT_S | OPT_STEP, WS, NUMBER}
